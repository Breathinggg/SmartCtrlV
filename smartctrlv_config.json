{
  "ui": {
    "language": "auto"
  },
  "explorer": {
    "enabled": true,
    "enable_command_from_clipboard": true,
    "enable_create_files_from_clipboard": true,
    "enable_write_file_from_clipboard": true,
    "text_ext_whitelist": [
      ".txt",
      ".md",
      ".json",
      ".py"
    ]
  },
  "menu": {
    "enabled": true,
    "whitelist_enabled": true,
    "whitelist_processes": [
      "notepad.exe",
      "chrome.exe",
      "code.exe"
    ],
    "hotkey": "ctrl+alt+v",
    "options": {
      "raw": false,
      "plain": false,
      "markdown": false,
      "structured": true,
      "collapse_blank": true,
      "python_dedent": true
    }
  },
  "hotkeys": {
    "explorer_ctrl_v": "ctrl+v"
  },
  "general": {
    "autostart": false
  }
}